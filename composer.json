{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/laravel-scout-elastic.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/art-institute-of-chicago/data-hub-foundation.git"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "aic/data-hub-foundation": "dev-master",
        "aic/laravel-scout-elastic": "dev-master#3.0.9-aic",
        "cviebrock/laravel-elasticsearch": "^2.0",
        "doctrine/dbal": "^2.6",
        "elasticsearch/elasticsearch": "^5.3",
        "fideloper/proxy": "~4.0",
        "jsq/amazon-es-php": "^0.2.0",
        "laravel/framework": "5.7.*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "~1.0",
        "league/commonmark": "^0.17.5",
        "league/csv": "^9.1",
        "league/fractal": "^0.16.0",
        "marijnvdwerf/material-palette": "^1.2",
        "marktopper/doctrine-dbal-timestamp-type": "^1.0",
        "sentry/sentry-laravel": "^0.10.0",
        "zendframework/zend-code": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "itsgoingd/clockwork": "^2.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
